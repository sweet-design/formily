{
  "name": "formily",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve-dev": "vue-cli-service serve --mode dev",
    "serve-sit": "vue-cli-service serve --mode sit",
    "serve-uat": "vue-cli-service serve --mode uat",
    "serve-mock": "vue-cli-service serve --mode mock",
    "serve-prod": "vue-cli-service serve --mode prod",
    "deploy:dev": "npm run build-dev && cross-env NODE_ENV=dev node ./deploy",
    "deploy:ut": "npm run build-ut && cross-env NODE_ENV=ut node ./deploy",
    "build": "vue-cli-service build --modern",
    "build-dev": "vue-cli-service build --mode dev --modern",
    "build-sit": "vue-cli-service build --mode sit --modern",
    "build-uat": "vue-cli-service build --mode uat --modern",
    "build-mock": "vue-cli-service build --mode mock --modern",
    "build-prod": "vue-cli-service build --mode prod --modern",
    "lint-nofix": "vue-cli-service lint --no-fix",
    "lint": "vue-cli-service lint",
    "view": "npx http-server ./dist -p 9090 -g",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "deploy": "gh-pages -d site",
    "prettier": "prettier --config .prettierrc --write **/*"
  },
  "dependencies": {
    "ant-design-vue": "1.7.3",
    "axios": "^0.21.1",
    "china-division": "^2.5.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "cui-page-designer": "^1.2.9",
    "dayjs": "^1.10.4",
    "intro.js": "^4.1.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "lodash.pick": "^4.4.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "nprogress": "^0.2.0",
    "querystring": "^0.2.1",
    "reqwest": "^2.0.5",
    "resize-observer-polyfill": "^1.5.1",
    "store": "^2.0.12",
    "superagent": "^6.1.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.24.1",
    "vue-introjs": "^1.3.2",
    "vue-ls": "^3.2.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "3.5.1",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.4.0",
    "vuex-module-decorators": "^1.0.1",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/classnames": "^2.2.11",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/nprogress": "^0.2.0",
    "@types/store": "^2.0.2",
    "@types/superagent": "^4.1.10",
    "@types/vue-ls": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "address": "^1.1.2",
    "babel-plugin-import": "^1.13.3",
    "clipboard": "^2.0.8",
    "compression-webpack-plugin": "4.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^4.0.0",
    "http-server": "^0.12.3",
    "less": "3.13.1",
    "less-loader": "7.3.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "scp2": "^0.5.0",
    "stylelint": "^13.10.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11",
    "vue-tsx-support": "^3.1.0",
    "vue2-ace-editor": "^0.0.15",
    "webpack-theme-color-replacer": "1.3.12"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{less,css}": [
      "npx stylelint --fix",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
